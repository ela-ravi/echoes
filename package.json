{
  "name": "react-typescript-webpack-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,scss,css,json,md}'",
    "format:fix": "yarn format",
    "prettify": "prettier --write .",
    "prettify:check": "prettier --check .",
    "server": "json-server-auth --watch db.json --port 3001",
    "dev": "concurrently \"yarn server\" \"yarn start\""
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@types/react-toastify": "^4.0.2",
    "copy-webpack-plugin": "6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.22.1",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.5",
    "@babel/preset-typescript": "^7.24.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.1.2",
    "css-loader": "^6.10.0",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "json-server": "0.17.4",
    "json-server-auth": "^2.1.0",
    "prettier": "^3.2.5",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^3.3.4",
    "typescript": "~5.3.3",
    "webpack": "^5.99.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
